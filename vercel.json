{
  "version": 2,
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/build/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/build/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ENABLE_VC_BUILD": "true"
  }
}